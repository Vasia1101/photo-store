{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "photos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::photo.photo",
      "mappedBy": "category"
    }
  }
}
