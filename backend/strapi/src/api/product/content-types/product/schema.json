{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "single, bundle"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "photos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::photo.photo",
      "mappedBy": "products"
    },
    "priceCents": {
      "type": "integer",
      "required": true
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "USD, EUR"
      ]
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isActive": {
      "type": "boolean",
      "required": false,
      "default": true
    }
  }
}
